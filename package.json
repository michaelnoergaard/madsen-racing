{
  "name": "madsen-racing",
  "type": "module",
  "version": "1.0.0",
  "description": "Personlig website for Anton Madsen - Danish Karting Driver",
  "scripts": {
    "dev": "astro dev --host 0.0.0.0",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "setup:contentful": "node scripts/setup-contentful.js",
    "setup:forside": "node scripts/create-forside-content.js",
    "setup:sponsors": "node scripts/setup-sponsor-packages.js",
    "setup:sections": "node scripts/setup-page-sections.js",
    "update:pagecontent": "node scripts/update-pagecontent.js",
    "update:pagecontent-buttons": "node scripts/update-pagecontent-buttons.js",
    "check:pagecontent": "node scripts/check-pagecontent.js",
    "check:published": "node scripts/check-published.js",
    "docker:build": "docker build -t madsen-racing .",
    "docker:run": "docker run -p 4321:4321 madsen-racing",
    "docker:local": "docker-compose up --build",
    "docker:local-detached": "docker-compose up --build -d",
    "docker:local-stop": "docker-compose down",
    "docker:local-logs": "docker-compose logs -f",
    "docker:local-rebuild": "docker-compose up --build --force-recreate"
  },
  "dependencies": {
    "@astrojs/check": "",
    "@astrojs/tailwind": "",
    "@contentful/rich-text-react-renderer": "^16.1.6",
    "astro": "",
    "contentful": "",
    "contentful-management": "",
    "dotenv": "",
    "pm2": "^6.0.14",
    "tailwindcss": "",
    "typescript": ""
  },
  "devDependencies": {
    "@types/node": ""
  }
}
